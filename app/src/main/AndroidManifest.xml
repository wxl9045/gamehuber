<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="safe.com.gamehuber">
    <!-- 访问网络 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="safe.com.gamehuber.MainActivity"/>
        <activity android:name="safe.com.gamehuber.MainActivity2"/>
        <activity android:name="safe.com.gamehuber.mvp.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="safe.com.gamehuber.mvp.login.ResetPasswordActivity"/>
        <activity android:name="safe.com.gamehuber.mvp.login.CreateAccountActivity"/>
        <activity android:name="safe.com.gamehuber.mvp.home.VideoDetailActivity"/>
        <activity android:name="safe.com.gamehuber.mvp.page.UserDetailActivity"/>
        <activity android:name="safe.com.gamehuber.mvp.page.UserDetail2Activity"/>
    </application>

</manifest>